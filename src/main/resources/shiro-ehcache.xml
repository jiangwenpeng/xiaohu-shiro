<!--https://www.cnblogs.com/sdream/p/5966668.html ehcache.xml配置详解-->
<ehcache>
    <defaultCache
            maxElementsInMemory="1000"
            eternal="false"
            timeToIdleSeconds="120"
            timeToLiveSeconds="120"
            memoryStoreEvictionPolicy="LRU"></defaultCache>
</ehcache>